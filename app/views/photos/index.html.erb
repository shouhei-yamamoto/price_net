<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>画像</th>
      <th>写真説明</th>
      <th>値段</th>
      <th>商品URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= image_tag photo.image.url %></td>
        <td><%= photo.content %></td>
        <td><% photo.products.each do |product| %><%= product.price %> <% end %></td>
        <td><% photo.product_urls.each do |product_url| %><%= product_url.price_url %><% end %></td>
        <td><% photo.labels.each do |label| %><%= label.name %><% end %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


