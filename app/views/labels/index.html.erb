<p id="notice"><%= notice %></p>

<h1>ラベル一覧</h1>
  <div class = "container-fluid tag">

      <p>ラベル</p>
      <th colspan="3"></th>
   

 
    <% @labels.each do |label| %>
      <div class = "row justify-content-around">
        <p><%= label.name %></p>
        <p><%= link_to '詳細', label %></p>
        <p><%= link_to '編集', edit_label_path(label) %></p>
        <p><%= link_to '削除', label, method: :delete %></p>
      </div>
    <% end %>
    
  </div>

<br>

<%= button_to 'ラベル登録', new_label_path,  method: :get, class: "btn btn-primary" %>
